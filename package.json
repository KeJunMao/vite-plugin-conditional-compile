{
  "name": "vite-plugin-conditional-compiler",
  "type": "module",
  "license": "MIT",
  "version": "0.1.1",
  "packageManager": "pnpm@8.8.0",
  "description": "",
  "homepage": "https://github.com/KeJunMao/vite-plugin-conditional-compile",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeJunMao/vite-plugin-conditional-compile.git"
  },
  "keywords": [
    "vite-plugin"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm build",
    "play": "vite ./playground",
    "release": "bumpp",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^20.7.1",
    "@vitest/coverage-c8": "^0.33.0",
    "bumpp": "^9.2.0",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "pnpm": "^8.8.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.5"
  },
  "dependencies": {
    "unplugin-preprocessor-directives": "^0.0.7"
  }
}
