{
  "name": "vite-plugin-conditional-compiler",
  "type": "module",
  "license": "MIT",
  "version": "0.1.1",
  "packageManager": "pnpm@7.28.0",
  "description": "",
  "homepage": "https://github.com/KeJunMao/vite-plugin-conditional-compile",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeJunMao/vite-plugin-conditional-compile.git"
  },
  "keywords": [
    "vite-plugin"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm build",
    "play": "vite ./playground",
    "release": "bumpp",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^18.14.4",
    "@vitest/coverage-c8": "^0.29.2",
    "bumpp": "^9.0.0",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "pnpm": "^7.28.0",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "dependencies": {
    "unplugin-preprocessor-directives": "^0.0.7"
  }
}
